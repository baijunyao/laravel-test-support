{
    "name": "baijunyao/laravel-test-support",
    "keywords": ["laravel", "test", "support"],
    "description": "Laravel Test Support",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "baijunyao",
            "email": "baijunyao@baijunyao.com"
        }
    ],
    "require": {
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^7.0|^8.0|^9.0",
        "baijunyao/laravel-support": "^1.3",
        "vlucas/phpdotenv": "^5.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "Baijunyao\\LaravelTestSupport\\": "src/"
        }
    },
    "scripts": {
        "phpcs": "vendor/bin/php-cs-fixer fix"
    }
}
